{
  "diffEditor.experimental.showMoves": true,
  "diffEditor.ignoreTrimWhitespace": false,
  "editor.defaultFormatter": "trunk.io",
  "editor.formatOnSave": true,
  "editor.inlineSuggest.enabled": true,
  "editor.renderControlCharacters": true,
  "editor.renderWhitespace": "all",
  "editor.wordWrap": "bounded",
  "editor.wordWrapColumn": 88,
  "git.confirmSync": false,
  "git.ignoreRebaseWarning": true,
  "githubPullRequests.ignoredPullRequestBranches": ["main"],
  "python.createEnvironment.trigger": "off",
  "python.defaultInterpreterPath": ".venv/bin/python",
  "python.envFile": "${workspaceFolder}/env/.env",
  "python.terminal.activateEnvironment": false,
  "python.testing.pytestArgs": ["tests", "--capture=no"],
  "python.testing.pytestEnabled": true,
  "python.testing.unittestEnabled": false,
  "testing.showAllMessages": true,
  "trunk.showPreexistingIssues": true,
  "files.associations": {
    "*.env": "shellscript",
    "*.sql": "jinja-sql",
    "*.toml": "toml"
  },
  "files.exclude": {
    "**/.git": false,
    "**/.trunk/*actions/": true,
    "**/.trunk/*logs/": true,
    "**/.trunk/*notifications/": true,
    "**/.trunk/*out/": true,
    "**/.trunk/*plugins/": true
  },
  "files.watcherExclude": {
    "**/.trunk/*actions/": true,
    "**/.trunk/*logs/": true,
    "**/.trunk/*notifications/": true,
    "**/.trunk/*out/": true,
    "**/.trunk/*plugins/": true
  },
  "[jsonc]": {
    "editor.defaultFormatter": "vscode.json-language-features"
  },
  "dbt.enableNewLineagePanel": true,
  "dbt.deferConfigPerProject": {
    "src/dbt/deanslist": {
      "manifestPathForDeferral": "/workspaces/teamster/src/dbt/deanslist/target",
      "manifestPathType": "local"
    },
    "src/dbt/edplan": {
      "manifestPathForDeferral": "/workspaces/teamster/src/dbt/edplan/target",
      "manifestPathType": "local"
    },
    "src/dbt/iready": {
      "manifestPathForDeferral": "/workspaces/teamster/src/dbt/iready/target",
      "manifestPathType": "local"
    },
    "src/dbt/kippcamden": {
