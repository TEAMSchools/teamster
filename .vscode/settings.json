{
  "dbt.enableNewLineagePanel": true,
  "dbt.enableNotebooks": false,
  "dbt.fileNameTemplateGenerateModel": "{prefix}_{sourceName}__{tableName}",
  "dbt.installDepsOnProjectInitialization": false,
  "dbt.prefixGenerateModel": "stg",
  "diffEditor.experimental.showMoves": true,
  "diffEditor.ignoreTrimWhitespace": false,
  "editor.formatOnSave": true,
  "editor.inlineSuggest.enabled": true,
  "editor.renderControlCharacters": true,
  "editor.renderWhitespace": "all",
  "editor.wordWrap": "bounded",
  "editor.wordWrapColumn": 88,
  "git.confirmSync": false,
  "git.ignoreRebaseWarning": true,
  "githubPullRequests.ignoredPullRequestBranches": ["main"],
  "python.createEnvironment.trigger": "off",
  "python.defaultInterpreterPath": "${workspaceFolder}/.venv/bin/python",
  "python.envFile": "${workspaceFolder}/env/.env",
  "python.terminal.activateEnvironment": false,
  "python.testing.pytestArgs": ["tests", "--capture=no"],
  "python.testing.pytestEnabled": true,
  "python.testing.unittestEnabled": false,
  "testing.showAllMessages": true,
  "trunk.autoInit": false,
  "trunk.enableUpgradePane": false,
  "trunk.showPreexistingIssues": true,
  "trunk.showWhatsNew": false,
  "trunk.trunkPath": "./trunk",
  "files.associations": {
    "*.env": "shellscript",
    "*.sql": "jinja-sql",
    "*.toml": "toml"
  },
  "files.exclude": {
    "**/.trunk/*actions/": true,
    "**/.trunk/*logs/": true,
    "**/.trunk/*notifications/": true,
    "**/.trunk/*out/": true,
    "**/.trunk/*plugins/": true
  },
  "files.watcherExclude": {
    "**/.trunk/*actions/": true,
    "**/.trunk/*logs/": true,
    "**/.trunk/*notifications/": true,
    "**/.trunk/*out/": true,
    "**/.trunk/*plugins/": true
  },
  "[jsonc]": {
    "editor.defaultFormatter": "trunk.io"
  },
  "[python]": {
    "editor.defaultFormatter": "trunk.io"
  },
  "terminal.integrated.env.linux": {
    "DAGSTER_HOME": "${workspaceFolder}/.dagster/home",
    "DBT_CLOUD_ENVIRONMENT_TYPE": "dev",
    "DBT_PROFILES_DIR": "${workspaceFolder}/.dbt",
    "GOOGLE_CLOUD_PROJECT": "teamster-332318",
    "PYTHONDONTWRITEBYTECODE": "1",
    "TRUNK_TELEMETRY": "off",
    "UV_ENV_FILE": "${workspaceFolder}/env/.env",
    "UV_LINK_MODE": "copy",
    "UV_RESOLUTION": "highest"
  },
  "yaml.schemas": {
    "https://raw.githubusercontent.com/dbt-labs/dbt-jsonschema/main/schemas/latest/dbt_yml_files-latest.json": [
      "${workspaceFolder}/src/dbt/**/*.yml",
      "!profiles.yml",
      "!dbt_project.yml",
      "!packages.yml",
      "!selectors.yml",
      "!profile_template.yml",
      "!package-lock.yml"
    ],
    "https://raw.githubusercontent.com/dbt-labs/dbt-jsonschema/main/schemas/latest/dbt_project-latest.json": [
      "dbt_project.yml"
    ],
    "https://raw.githubusercontent.com/dbt-labs/dbt-jsonschema/main/schemas/latest/selectors-latest.json": [
      "selectors.yml"
    ],
    "https://raw.githubusercontent.com/dbt-labs/dbt-jsonschema/main/schemas/latest/packages-latest.json": [
      "packages.yml"
    ]
  },
  "dbt.allowListFolders": [
    "src/dbt/kipptaf",
    "src/dbt/kippcamden",
    "src/dbt/kippmiami",
    "src/dbt/kippnewark",
    "src/dbt/kipppaterson"
  ],
  "dbt.perspectiveTheme": "Vintage"
}
