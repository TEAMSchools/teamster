{
  "dbt.versionCheck": "status bar",
  "dbt.prefixGenerateModel": "stg",
  "editor.defaultFormatter": "trunk.io",
  "editor.formatOnSave": true,
  "editor.renderWhitespace": "all",
  "editor.inlineSuggest.enabled": true,
  "editor.renderControlCharacters": true,
  "editor.wordWrap": "bounded",
  "editor.wordWrapColumn": 88,
  "files.associations": {
    "*.toml": "toml",
    "*.env": "shellscript",
    "*.sql": "jinja-sql"
  },
  "files.exclude": {
    "**/.git": false,
    "**/.trunk/*out": true
  },
  "files.watcherExclude": {
    "**/.trunk/*out": true
  },
  "python.analysis.autoImportCompletions": false,
  "python.defaultInterpreterPath": "/workspaces/teamster/.venv/bin/python",
  "python.terminal.activateEnvironment": false,
  "python.analysis.typeCheckingMode": "off",
  "python.testing.pytestArgs": ["tests"],
  "python.testing.unittestEnabled": false,
  "python.testing.pytestEnabled": true,
  "[json]": {
    "editor.defaultFormatter": "vscode.json-language-features"
  },
  "githubPullRequests.ignoredPullRequestBranches": ["main"],
  "dbt.fileNameTemplateGenerateModel": "{prefix}_{sourceName}__{tableName}",
  "dbt.queryLimit": 500,
  "trunk.numJobs": 1
}
