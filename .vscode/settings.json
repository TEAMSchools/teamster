{
  "diffEditor.experimental.showMoves": true,
  "diffEditor.ignoreTrimWhitespace": false,
  "editor.defaultFormatter": "trunk.io",
  "editor.formatOnSave": true,
  "editor.inlineSuggest.enabled": true,
  "editor.renderControlCharacters": true,
  "editor.renderWhitespace": "all",
  "editor.wordWrap": "bounded",
  "editor.wordWrapColumn": 88,
  "git.confirmSync": false,
  "git.ignoreRebaseWarning": true,
  "githubPullRequests.ignoredPullRequestBranches": ["main"],
  "python.createEnvironment.trigger": "off",
  "python.defaultInterpreterPath": ".venv/bin/python",
  "python.envFile": "${workspaceFolder}/env/.env",
  "python.terminal.activateEnvironment": false,
  "python.testing.pytestArgs": ["tests", "--capture=no"],
  "python.testing.pytestEnabled": true,
  "python.testing.unittestEnabled": false,
  "testing.showAllMessages": true,
  "trunk.showPreexistingIssues": true,
  "files.associations": {
    "*.env": "shellscript",
    "*.sql": "jinja-sql",
    "*.toml": "toml"
  },
  "files.exclude": {
    "**/.trunk/*actions/": true,
    "**/.trunk/*logs/": true,
    "**/.trunk/*notifications/": true,
    "**/.trunk/*out/": true,
    "**/.trunk/*plugins/": true
  },
  "files.watcherExclude": {
    "**/.trunk/*actions/": true,
    "**/.trunk/*logs/": true,
    "**/.trunk/*notifications/": true,
    "**/.trunk/*out/": true,
    "**/.trunk/*plugins/": true
  },
  "[jsonc]": {
    "editor.defaultFormatter": "vscode.json-language-features"
  },
  "dbt.enableNewLineagePanel": true,
  "[python]": {
    "editor.defaultFormatter": "trunk.io"
  },
  "terminal.integrated.env.linux": {
    "DAGSTER_HOME": "/workspaces/teamster/.dagster/home",
    "DBT_CLOUD_ENVIRONMENT_TYPE": "dev",
    "DBT_DEV_CLOUD_STORAGE_URI_BASE": "gs://teamster-test/dagster/kippnewark",
    "DBT_PROFILES_DIR": "/workspaces/teamster/.dbt",
    "PYTHONDONTWRITEBYTECODE": "1",
    "TRUNK_TELEMETRY": "off",
    "UV_ENV_FILE": "/workspaces/teamster/env/.env",
    "UV_LINK_MODE": "copy",
    "UV_RESOLUTION": "highest"
  },
  "dbt.deferConfigPerProject": {
    "src/dbt/kipptaf": {
      "deferToProduction": true
    }
  }
}
